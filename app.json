{
  "expo": {
    "name": "movie-reviewer",
    "slug": "movie-reviewer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/resizedpopcorn.png",
    "splash": {
      "image": "./assets/resizeblue.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/resizedpopcorn.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Simple CRUD App where a user can add/edit movies and make reviews to a backend API.",
    "githubUrl": "https://github.com/katiepestotnik/reviewer-frontend"
  }
}
